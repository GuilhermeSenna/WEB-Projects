<%- include('partials/header') %>


    <h1>Home</h1>
    <p>
        <%= homeText %>
    </p>

	<% postsHome.forEach((post) => { %>
		<h1>
			<%= post.title, 100 %>
		</h1>
		<p>
			<%= post.content.slice(0, 100) + " ..." %>
			<a href="posts/<%= post.title %> ">Read More</a>
		</p>
	<% }); %>

<%- include('partials/footer') %>